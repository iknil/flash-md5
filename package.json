{
  "name": "flash-md5",
  "version": "1.0.0",
  "description": "FlashMD5 is a fast MD5 tool for WebApp, especially for scenarios where you need to calculate the MD5 of large files.",
  "main": "lib/index.umd.js",
  "module": "lib/index.js",
  "scripts": {
    "test": "serve test",
    "build": "rollup -c",
    "commit": "cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iknil/flash-md5.git"
  },
  "keywords": [
    "MD5",
    "web",
    "fast"
  ],
  "files": ["/lib", "README.md", "README_CN.md"],
  "author": "iknil",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iknil/flash-md5/issues"
  },
  "homepage": "https://github.com/iknil/flash-md5#readme",
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@commitlint/config-conventional": "^19.8.1",
    "commitizen": "^4.3.1",
    "commitlint": "^19.8.1",
    "conventional-changelog-cli": "^5.0.0",
    "cz-customizable": "^7.4.0",
    "husky": "^9.1.7",
    "rollup": "^2.47.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-dts": "^4.2.3",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript": "^1.0.1",
    "serve": "^14.2.4",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  }
}
